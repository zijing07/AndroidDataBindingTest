<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zijing.com.databindingtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".newway.MainActivity"></activity>
        <activity android:name=".newway.RecyclerViewActivity"></activity>
        <activity android:name=".newway.SeekBarActivity"></activity>
        <activity android:name=".newway.DisplayModelActivity"></activity>

        <activity android:name=".oldway.OldMainActivity"></activity>
        <activity android:name=".oldway.OldSeekBarActivity"></activity>
        <activity android:name=".oldway.OldRecyclerViewActivity"></activity>
        <activity android:name=".oldway.OldDisplayModelActivity"></activity>
    </application>

</manifest>